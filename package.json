{
  "name": "Whattheflux",
  "repository": "https://github.com/yhbrandon/whattheflux",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "dev": "better-npm-run dev",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node -- bin/compile.js"
    },
    "dev": {
      "command": "nodemon --exec babel-node --harmony bin/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "yarn": "^0.18.1"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.10",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "immutability-helper": "^2.0.0",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^0.10.1",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-material-flexbox": "^1.0.9",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.7",
    "react-toolbox": "^1.3.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13",
    "webpack-merge": "^0.14.1"
  }
}

{
  "name": "what_the_flux",
  "repository": "https://github.com/yhbrandon/whattheflux",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "build:dev": "better-npm-run dev",
    "build:prod": "better-npm-run prod",
    "test": "better-npm-run test",
    "lint": "better-npm-run lint",
    "lint:js": "better-npm-run js",
    "lint:sass": "better-npm-run sass"
  },
  "author": "Brandon Clark",
  "license": "MIT",
  "betterScripts": {
    "compile": {
      "command": "babel-node -- ./node_modules/.bin/webpack -p --config config/webpack/webpack.prod.config.js --progress --colors"
    },
    "dev": {
      "command": "nodemon bin/server.js --exec babel-node --harmony",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.10",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.22.0",
    "immutability-helper": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^0.10.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-material-flexbox": "^1.0.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-toolbox": "^1.1.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13",
    "webpack-merge": "^0.14.1"
  }
}
